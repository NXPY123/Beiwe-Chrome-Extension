{
   "manifest_version": 3,
   "name": "Beiwe",
   "version": "0.1",
   "description": "Beiwe",
   "permissions":[
      "cookies"
   ],
   "host_permissions": [
      "https://beiwe.herokuapp.com"
   ],
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "js/jquery-3.6.0.min.js", "js/script.js"
         ],
         "css": [
            "css/style.css"
         ],
         "all_frames": true,
         "run_at": "document_end"
      }
   ],
   "background": {
      "service_worker": "js/background.js"
   },
   "action":{
      "default_icon": "assets/icon.png",
      "default_popup": "popup.html"
   }
}